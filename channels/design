<div id="corners2">
<div id="3">
<textarea id="t6" rows="1">Lab3 </textarea>
<button id="b6"> + </button>
<script>
function findEmptyDiv(){
 for (i=1; i<6; i++){
  var elem = $('#d'+i);
  if (elem.is(':empty')){
   return elem;
  }
 }
 return null;
}
 
    $("#b6").click(function(){
        var elem = findEmptyDiv();
        if(elem != null) {
         elem.replaceWith('<div id="#3"><label>'+ $("#t6").val() +' </label><button id="b3">-</button></div>');
       }
    });
</script>
</div>
</div>
</body>
</html>


